  "version": "0.0.4",
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "icon.png",
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:srcmark.toggle",
    "onCommand:srcmark.runNow"
  ],
  "contributes": {
    "commands": [
      {
        "command": "srcmark.toggle",
        "title": "SrcMark: Toggle On/Off"
      },
      {
        "command": "srcmark.runNow",
        "title": "SrcMark: Run Now (prepend to active editor)"
      }
    ],
    "configuration": {
      "title": "SrcMark",
      "properties": {
        "srcmark.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable SrcMark at activation. You can toggle at runtime with the command."
        },
        "srcmark.autoDetectComment": {
          "type": "boolean",
          "default": true,
          "description": "Automatically detect and use the appropriate comment syntax for each file type. Disable to use commentPrefix for all files."
        },
        "srcmark.commentPrefix": {
          "type": "string",
          "default": "//",
          "description": "Comment prefix to use when autoDetectComment is disabled, or as fallback for unknown file types."
        },
        "srcmark.fileGlob": {
          "type": "string",
          "default": "**/*",
          "description": "Glob pattern of files to apply SrcMark to (uses vscode glob semantics). Default applies to all files."
        },
        "srcmark.skipIfPresent": {
          "type": "boolean",
          "default": true,
          "description": "If true, SrcMark will not add the comment if an identical top-line comment already exists."
        }
      }
    }
  },
  "description": "Minimal VS Code extension that prepends a top-line comment with the path relative to the workspace root (or filename in single-file mode).",
  "directories": {
    "test": "test"
  },
